{
  "name": "Flickr Bundle Downloader",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.flickr.com/**/*"],
      "js": ["polyfills/webcomponents-bundle.js", "./index.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {}
}
